<h1 class="text-2xl font-bold mb-4">Available Tests</h1>
<div class="grid grid-cols-1 md:grid-cols-2 gap-4">
  <% tests.forEach(test => { %>
    <div class="p-4 border rounded-lg shadow hover:shadow-lg">
      <h2 class="text-xl font-semibold mb-2"><%= test.title %> (<%= test.level %>)</h2>
      <p>Total Questions: <%= test.questions.length %></p>
      <a href="/dashboard/test/start/<%= test._id %>" class="mt-2 inline-block px-4 py-2 bg-teal-500 text-white rounded hover:bg-teal-600">Start Test</a>
    </div>
  <% }) %>
</div>
